{
  "name": "electron-updater-example",
  "version": "0.4.6",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "devDependencies": {
    "electron": "^1.7.8",
    "electron-builder": "^19.31.1"
  },
  "dependencies": {
    "electron-log": "^2.2.9",
    "electron-updater": "^2.10.0"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "lgeiger",
        "repo": "electron-updater-example"
      }
    ],
    "appId": "com.github.lgeiger.electronupdaterexample",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "perMachine": true
    }
  }
}
